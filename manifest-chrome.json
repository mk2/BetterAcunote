{
	"name": "BetterAcunote",
	"description": "Chrome extension for more better Acunote UX",
	"homepage_url": "https://github.com/mk2/betteracunote",
	"icons": {
		"128": "icon-128.png"
	},
	"content_scripts": [{
		"run_at": "document_start",
		"matches": ["*://*.acunote.com/*"],
		"css": ["betteracunote.css"],
		"js": ["betteracunote.js"]
	}],
	"permissions": [
		"tabs",
		"*://*.acunote.com/*"
	],
	"version": "0.1",
	"manifest_version": 2
}
